            ds_grid_read(lnz, ds_list_find_value(fotos, i));
            for (var w = 0; w &lt; ww; w++) {
                for (var h = 0; h &lt; hh; h++) {
                    buffer_write(buf, buffer_u8, ds_grid_get(lnz, w, h));
                }
            }
        }
        ds_grid_destroy(lnz);
        s_udp_send(buf, "");
    }
}

