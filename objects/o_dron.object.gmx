        vel = min(vel, o_control.velocidad * 1.1) * dlt;
        direction = point_direction(x, y, mx, my);
        x += lengthdir_x(vel, direction);
        y += lengthdir_y(vel, direction);
    }
    
    // prevenir salirse del mundo
    x = clamp(x, 0, room_width);
    y = clamp(y, 0, room_height);
    depth = -y;
}

</string>
