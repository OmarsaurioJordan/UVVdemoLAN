        if otr.object_index == o_solido {
            if otr.imagen == d_estructura {
                switch otr.subimagen {
                    case 1: case 2: // puerta
                        if altura &lt; 50 {
                            otr = noone;
                        }
                        break;
                    case 3: case 4: // ventana
                        if altura &gt; 40 and altura &lt; 50 {
                            otr = noone;
                        }
                        break;
                    case 5: case 6: // muro
                        if altura &gt; 40 {
                            otr = noone;
                        }
                        break;
                }
            }
        }
        else if otr.object_index == o_monumento {
            if altura &gt; 30 {
                otr = noone;
            }
        }
