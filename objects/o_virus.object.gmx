        infectados = 0;
        muertos = 0;
        poblacion = 0;
        var yop = id;
        with o_usuario {
            yop.poblacion++;
            if salud == 1 {
                yop.muertos++;
            }
            else if salud != 0 {
                yop.infectados++;
            }
        }
        if poblacion != 0 {
            infectados = ceil((infectados / poblacion) * 100);
            muertos = ceil((muertos / poblacion) * 100);
        }
    }
}

