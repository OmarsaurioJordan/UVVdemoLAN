        var lao = ds_grid_width(barcos);
        var buf = s_header(42, 6 + lao * lao * 2);
        buffer_write(buf, buffer_u16, idweb);
        buffer_write(buf, buffer_u32, fecha);
        for (var h = 0; h &lt; lao; h++) {
