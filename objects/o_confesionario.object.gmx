        s_udp_send(buf, "");
    }
    
    // ver si hacer la confesion
    var t;
    for (var i = 0; i &lt; ds_list_size(textos); i++) {
        t = ds_list_find_value(tiempos, i);
        if t != 0 {
            t = max(0, t - dlt);
            ds_list_replace(tiempos, i, t);
            if t == 0 {
                leido = false;
            }
